  "commands": {
    "Level": {
      "menu": "Memory",
      "callback": "setLevel",
      "parameters": {
        "Channel Type": {
          "type": "Enum",
          "options": {
            "Fader": 0,
            "Live": 124,
            "Next": 125,
            "Bump Master": 126,
            "Grand Master": 127
          }
        },
        "Channel": {
          "type": "Integer",
          "min": 1,
          "max": 24,
          "dependency": {
            "source": "channelType",
            "value": 0,
            "check": "equals",
            "action": "show"
          }
        },
        "Level": {
          "type": "Integer",
          "min": 0,
          "max": 127,
          "default": 127
        }
      }
    },
    "Bump": {
      "menu": "Memory",
      "callback": "bump",
      "parameters": {
        "Channel Number": {
          "type": "Integer",
          "min": 1,
          "max": 48
        }
      }
    },
    "Crossfade": {
      "menu": "Memory",
      "callback": "crossfade",
      "parameters": {
        "From": {
          "type": "Integer",
          "min": 1,
          "max": 24
        },
        "To": {
          "type": "Integer",
          "min": 1,
          "max": 24
        },
        "Time": {
          "type": "Float",
          "description": "Fade in/out time in seconds",
          "min": 0
        }
      }
    },
    "Memory Page": {
      "menu": "Memory",
      "callback": "setPage",
      "parameters": {
        "Page": {
          "type": "Integer",
          "min": 1,
          "max": 12
        }
      }
    },
    "Reset": {
      "menu": "Stack",
      "callback": "resetStack"
    },
    "Goto": {
      "menu": "Stack",
      "callback": "goStack",
      "parameters": {
        "Cue": {
          "type": "Integer",
          "min": 1,
          "max": 99
        }
      }
    },
    "Pause": {
      "menu": "Stack",
      "callback": "pauseStack"
    },
    "Start/GO": {
      "menu": "Stack",
      "callback": "startStack"
    },
    "Blackout": {
      "menu": "Stack",
      "callback": "blackout",
      "parameters": {
        "On": {
          "type": "Boolean"
        }
      }
    }
